---
import BaseLayout from "../layouts/BaseLayout.astro";
const allPosts = await Astro.glob("./posts/*.md");

---
<BaseLayout title="Blog">
		<h1>Our Blog Page</h1>
        <h2>Teehe</h2>
        <ul>
         {allPosts.map(post => <li><a href={post.url}>{post.frontmatter.title}</a></li>)}
        </ul>
</BaseLayout>